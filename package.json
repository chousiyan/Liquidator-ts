    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.23.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "phaser": "^3.54.0",
    "phaser3-weapon-plugin": "^2.1.0"
  },
  "parcelCleanPaths": [
    "dist"
  ],
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
